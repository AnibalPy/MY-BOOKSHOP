{
  "name": "my-bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.15.0",
    "@cap-js/sqlite": "^1",
    "@sap/ux-specification": "^1.136.9",
    "@types/node": "^24.10.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-browserbooks": "cds watch --open browserbooks/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db/my-bookshop.sqlite"
        }
      }
    },
    "fiori": {
      "lean_draft": true,
      "preview": {
        "ui5_version": "latest"
      }
    },
    "features": {
      "fetch_csrf": true,
      "assert_integrity": "db"
    },
    "protocols": {
      "odata": {
        "v4": {
          "endpoint": "/odata/v4"
        }
      }
    },
    "log": {
      "levels": {
        "odata": "off",
        "http": "off"
      }
    }
  },
  "sapux": [
    "app/browserbooks"
  ]
}
